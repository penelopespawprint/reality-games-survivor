{
  "name": "rgfl-survivor",
  "version": "1.0.0",
  "description": "Reality Games Fantasy League: Survivor",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "web",
    "mobile"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=web",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "npm run build --workspace=web",
    "build:server": "cd server && npm run build",
    "dev:mobile": "npm run start --workspace=mobile",
    "prepare": "husky"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "node-cron": "^3.0.3",
    "resend": "^2.0.0",
    "stripe": "^14.0.0",
    "twilio": "^5.11.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@vercel/node": "^3.0.0",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
